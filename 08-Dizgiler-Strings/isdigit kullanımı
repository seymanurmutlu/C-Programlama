#include <stdio.h>
#include <ctype.h> //isdigit ctype kutuphanesinde bulunur.
int main(int argc, const char * argv[]) {
    
    char c1;
    
    printf("karakter gir: ");
    scanf("%c",&c1);
    
       if(isdigit(c1)) // rakam mı? hazır fonksiyonu.
        printf("%c karakteri bir rakamdir.\n",c1);
    
     else
        printf("%c karakteri bir rakam degildir.\n",c1);
        
return 0;
}
