#include <stdio.h>

int main(int argc, const char * argv[]) {
    char tel[50];
    char *ptr;
    
    printf("Bir telefon numarasi giriniz: ");
    scanf("%s",tel);
    printf("Ayrilmis hali: \n");
    
    ptr=strtok(tel,"-");

    printf("Ulke kodu: %s \n",ptr);
    ptr=strtok(NULL,"-");
    
    printf("Alan kodu: %s \n",ptr);
    ptr=strtok(NULL,"-");
    
    printf("telefon numarasi: %s \n",ptr);
    
    
    return 0;
}
