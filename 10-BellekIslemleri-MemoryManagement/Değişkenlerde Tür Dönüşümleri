#include <stdio.h>

int main(int argc, const char * argv[]) {
    float x=3.5;
    
    int y=(int)x; //değişkenlerde tür dönüşümleri 
    
    printf("x = %f\n",x);
    printf("y = %d\n",y);
    
    return 0;
}
