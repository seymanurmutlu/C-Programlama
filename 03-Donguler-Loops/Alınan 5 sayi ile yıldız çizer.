#include <stdio.h>
#include <stdlib.h>
int main()
{
    int s1,s2,s3,s4,s5,i;
    printf("5 adet sayi giriniz:");
    scanf("%d%d%d%d%d",&s1,&s2,&s3,&s4,&s5);

    for(i=0;i<s1;i++){
        printf("*");
        }
    printf("\n");

    for(i=0;i<s2;i++){
        printf("*");
        }
    printf("\n");
    for(i=0;i<s3;i++){
        printf("*");
        }
    printf("\n");
    for(i=0;i<s4;i++){
        printf("*");
        }
    printf("\n");
    for(i=0;i<s5;i++){
        printf("*");
        }
    printf("\n");
    return 0;
}
