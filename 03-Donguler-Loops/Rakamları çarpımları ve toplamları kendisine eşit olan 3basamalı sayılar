#include <stdio.h>

int main(int argc, const char * argv[]) {
    int i,j=0;
    int bir,on,yuz;
    
    
    for(i=100;i<1000;i++){
        bir=i%10;
        on=(i/10)%10;
        yuz=i/100;
        if(i==bir*on*yuz*(bir+on+yuz)){
            printf("%d ",i);
            j++;
        }
        
    }
    printf("\n");
    printf("%d tane\n\n",j);
    return 0;
}
